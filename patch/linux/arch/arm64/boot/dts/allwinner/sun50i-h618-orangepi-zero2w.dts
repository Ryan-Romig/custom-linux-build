/dts-v1/;

/ {
	#address-cells = <0x02>;
	model = "OrangePi Zero2 W";
	serial-number = "33802000c46ac590";
	#size-cells = <0x02>;
	interrupt-parent = <0x01>;
	compatible = "xunlong,orangepi-zero2w\allwinner,sun50i-h616\allwinner,sun50i-h618";

	vcc5v {
		regulator-max-microvolt = <0x4c4b40>;
		regulator-always-on;
		regulator-min-microvolt = <0x4c4b40>;
		regulator-name = "vcc-5v";
		compatible = "regulator-fixed";
		phandle = <0x52>;
	};

	connector {
		type = "d";
		compatible = "hdmi-connector";

		port {

			endpoint {
				remote-endpoint = <0x51>;
				phandle = <0x3f>;
			};
		};
	};

	gpu-opp-table {
		compatible = "operating-points-v2";
		phandle = <0xa2>;

		opp-432000000 {
			opp-microvolt = <0xc5c10>;
			opp-hz = <0x00 0x19bfcc00>;
		};

		opp-125000000 {
			opp-microvolt = <0xc5c10>;
			opp-hz = <0x00 0x7735940>;
		};

		opp-250000000 {
			opp-microvolt = <0xc5c10>;
			opp-hz = <0x00 0xee6b280>;
		};

		opp-600000000 {
			opp-microvolt = <0xea600>;
			opp-hz = <0x00 0x23c34600>;
		};

		opp-800000000 {
			opp-microvolt = <0x107ac0>;
			opp-hz = <0x00 0x2faf0800>;
		};
	};

	wifi-pwrseq {
		clock-names = "osc32k-out";
		clocks = <0x10 0x01>;
		reset-gpios = <0x1e 0x06 0x12 0x01>;
		compatible = "mmc-pwrseq-simple";
		post-power-on-delay-ms = <0xc8>;
		phandle = <0x22>;
	};

	thermal-zones {

		cpu-thermal {
			polling-delay = <0x00>;
			polling-delay-passive = <0x00>;
			thermal-sensors = <0x4c 0x02>;

			trips {

				cpu-crit {
					temperature = <0x186a0>;
					hysteresis = <0x00>;
					type = "critical";
				};

				cpu-alert {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x4d>;
				};
			};

			cooling-maps {

				map0 {
					trip = <0x4d>;
					cooling-device = <0x06 0xffffffff 0xffffffff 0x07 0xffffffff 0xffffffff 0x08 0xffffffff 0xffffffff 0x09 0xffffffff 0xffffffff>;
				};
			};
		};

		ddr-thermal {
			polling-delay = <0x00>;
			polling-delay-passive = <0x00>;
			thermal-sensors = <0x4c 0x03>;

			trips {

				ddr-alert {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0xa1>;
				};
			};
		};

		ve-thermal {
			polling-delay = <0x00>;
			polling-delay-passive = <0x00>;
			thermal-sensors = <0x4c 0x01>;

			trips {

				ve-alert {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0xa0>;
				};
			};
		};

		gpu-thermal {
			polling-delay = <0x00>;
			polling-delay-passive = <0x00>;
			thermal-sensors = <0x4c 0x00>;

			trips {

				gpu-alert {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x4e>;
				};

				gpu-crit {
					temperature = <0x186a0>;
					hysteresis = <0x00>;
					type = "critical";
				};
			};

			cooling-maps {

				map0 {
					trip = <0x4e>;
					cooling-device = <0x4f 0xffffffff 0xffffffff>;
				};
			};
		};
	};

	soc {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		compatible = "simple-bus";
		ranges = <0x00 0x00 0x00 0x40000000>;

		phy@5100400 {
			clock-names = "usb0_phy\0usb1_phy\0usb2_phy\0usb3_phy\0pmu2_clk";
			reg-names = "phy_ctrl\0pmu0\0pmu1\0pmu2\0pmu3";
			resets = <0x02 0x26 0x02 0x27 0x02 0x28 0x02 0x29>;
			clocks = <0x02 0x61 0x02 0x63 0x02 0x65 0x02 0x67 0x02 0x6e>;
			#phy-cells = <0x01>;
			compatible = "allwinner,sun50i-h616-usb-phy";
			status = "okay";
			reg = <0x5100400 0x24 0x5101800 0x14 0x5200800 0x14 0x5310800 0x14 0x5311800 0x14>;
			phandle = <0x3c>;
			reset-names = "usb0_reset\0usb1_reset\0usb2_reset\0usb3_reset";
		};

		gpu@1800000 {
			mali-supply = <0x0d>;
			clock-names = "core\0bus";
			resets = <0x02 0x03>;
			interrupts = <0x00 0x5f 0x04 0x00 0x60 0x04 0x00 0x61 0x04>;
			clocks = <0x02 0x23 0x02 0x24>;
			compatible = "allwinner,sun50i-h616-mali\0arm,mali-bifrost";
			status = "disabled";
			interrupt-names = "job\0mmu\0gpu";
			reg = <0x1800000 0x40000>;
			phandle = <0x4f>;
		};

		pwm0@0300a000 {
			compatible = "allwinner,sunxi-pwm0";
			phandle = <0x11>;
		};

		syscon@3000000 {
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			compatible = "allwinner,sun50i-h616-system-control";
			ranges;
			reg = <0x3000000 0x30 0x3000038 0xfc8>;
			phandle = <0x39>;

			sram@1a00000 {
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				compatible = "mmio-sram";
				ranges = <0x00 0x1a00000 0x200000>;
				reg = <0x1a00000 0x200000>;
				phandle = <0x59>;

				sram-section@0 {
					compatible = "allwinner,sun50i-h616-sram-c1";
					reg = <0x00 0x200000>;
					phandle = <0x0e>;
				};
			};

			sram@100000 {
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				compatible = "mmio-sram";
				ranges = <0x00 0x100000 0x18000>;
				reg = <0x100000 0x18000>;
				phandle = <0x56>;

				scpi-sram@17c00 {
					compatible = "arm,scp-shmem";
					reg = <0x17c00 0x200>;
					phandle = <0x57>;
				};
			};

			sram@28000 {
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				compatible = "mmio-sram";
				ranges = <0x00 0x28000 0x30000>;
				reg = <0x28000 0x30000>;
				phandle = <0x58>;

				sram-section@0 {
					compatible = "allwinner,sun50i-h616-sram-c\0allwinner,sun50i-a64-sram-c";
					reg = <0x00 0x1e000>;
					phandle = <0x0a>;
				};
			};
		};

		mmc@4022000 {
			mmc-ddr-3_3v;
			pinctrl-names = "default";
			#address-cells = <0x01>;
			pinctrl-0 = <0x23>;
			clock-names = "ahb\0mmc";
			cap-sd-highspeed;
			resets = <0x02 0x10>;
			cap-sdio-irq;
			interrupts = <0x00 0x25 0x04>;
			clocks = <0x02 0x41 0x02 0x3e>;
			#size-cells = <0x00>;
			compatible = "allwinner,sun50i-h616-emmc\0allwinner,sun50i-a100-emmc";
			status = "disabled";
			reg = <0x4022000 0x1000>;
			phandle = <0x6c>;
			max-frequency = <0x8f0d180>;
